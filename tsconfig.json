{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl":"./",
    "paths":{
      "@assets/*": 
      ["./src/assets"],
      "@components/*": 
      [
      "./src/components/*"
      ],
      "@routes/*": 
      ["./src/routes/*"],
      "@interfaces/*": 
      ["./src/screens/*"],
      "@storage/*": 
      ["./src/storage/*"],
      "@themes/*": 
      ["./src/theme/*"],
      "@utils/*": 
      ["./src/utils/*"]
  }
  }
}
